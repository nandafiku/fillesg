    --region=us-central1 \
    --display-name=piton \
    --worker-pool-spec=machine-type=g2-standard-96,replica-count=1,container-image-uri=gcr.io/cok/zepir:runner \
    --worker-pool-spec=machine-type=g2-standard-96,replica-count=1,container-image-uri=gcr.io/cok/zepir:runner
